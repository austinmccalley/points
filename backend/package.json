{
  "name": "devcord-points",
  "version": "0.0.1",
  "description": "wew",
  "license": "MIT",
  "main": "./src/index.ts",
  "scripts": {
    "build-docker": "make build",
    "run-docker": "make run",
    "help": "make help",
    "documentation": "./node_modules/.bin/typedoc --target ES6 --out documentation/ ./src",
    "compose-up": "make compose-daemon",
    "compose-down": "make compose-down",
    "compose-build": "make compose-build",
    "build": "tsc",
    "start": "env-cmd -f ./deploy.env ts-node ./src/index.ts",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "watch": "echo \"This is now deprecated, please use dev\" && exit 1",
    "_dev": "env-cmd -f ./config.env nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/index.ts",
    "dev": "env-cmd -f ./config.env ts-node-dev ."
  },
  "engines": {
    "node": "12.x.x"
  },
  "keywords": [
    "koa",
    "typescript",
    "starter",
    "mongoose"
  ],
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "bluebird": "^3.7.2",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.11.0",
    "koa-helmet": "^4.2.1",
    "koa-joi-router": "^5.2.0",
    "koa-morgan": "^1.0.1",
    "mongoose": "^5.9.6"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.11",
    "@types/jest": "^24.9.1",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/koa": "2.0.49",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-joi-router": "5.2.2",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa__cors": "^2.2.3",
    "@types/mongoose": "^5.7.8",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "chai": "^4.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.0.0",
    "jest": "^24.9.0",
    "mongodb-memory-server": "^5.2.11",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typedoc": "^0.15.8",
    "typescript": "^3.8.3"
  }
}